{
  "name": "monolithic-react-fastify-app",
  "version": "1.0.0",
  "description": "A monolithic React application with a Fastify backend.",
  "main": "src/server/index.js",
  "scripts": {
    "client": "webpack --config webpack.client.js --watch",
    "server": "nodemon --watch src/server --ext ts --exec ts-node src/server/index.ts",
    "tsc:watch": "tsc --noEmit --watch",
    "dev:strict": "concurrently \"npm run tsc:watch\" \"npm run client\" \"npm run server\"",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "build": "webpack --config webpack.client.js --mode production"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@fastify/static": "8.2.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "7.1.1",
    "@mui/x-data-grid": "8.5.2",
    "@prisma/client": "6.9.0",
    "axios": "1.9.0",
    "dotenv": "16.5.0",
    "fastify": "5.4.0",
    "music-metadata": "11.2.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.27.1",
    "@types/node": "24.0.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/webpack": "5.28.5",
    "babel-loader": "10.0.0",
    "concurrently": "9.1.2",
    "css-loader": "7.1.2",
    "nodemon": "3.1.10",
    "prisma": "6.9.0",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "webpack": "5.99.9",
    "webpack-cli": "6.0.1"
  },
  "keywords": [],
  "author": "Kshitij Murabatte",
  "license": "ISC"
}
